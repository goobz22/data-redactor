      "John Doe",
      "Jane Smith",
      "Bob Johnson"
    ]
  },
  "patterns": {
    "ipAddress": {
      "enabled": true,
      "strategy": "formatPreserving"
    },
    "macAddress": {
      "enabled": true,
      "strategy": "formatPreserving"
    },
    "email": {
      "enabled": true,
      "strategy": "formatPreserving"
    },
    "phone": {
      "enabled": true,
      "strategy": "formatPreserving"
    },
    "ssn": {
      "enabled": true,
      "strategy": "mask"
    },
    "creditCard": {
      "enabled": true,
      "strategy": "mask"
    },
    "hostname": {
      "enabled": true,
